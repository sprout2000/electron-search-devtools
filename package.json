{
  "name": "electron-search-devtools",
  "description": "Locate the developer tools for Electron.",
  "version": "1.8.0",
  "license": "MIT",
  "author": {
    "name": "sprout2000"
  },
  "homepage": "https://github.com/sprout2000/electron-search-devtools#readme",
  "repository": {
    "url": "https://github.com/sprout2000/electron-search-devtools"
  },
  "bugs": {
    "url": "https://github.com/sprout2000/electron-search-devtools/issues"
  },
  "keywords": [
    "npm",
    "esm",
    "vue",
    "react",
    "redux",
    "nodejs",
    "electron",
    "typescript",
    "developer tools"
  ],
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "engines": {
    "node": ">=10.13.0 || >=12.13.1"
  },
  "scripts": {
    "compile": "run-s build minify",
    "prebuild": "rimraf dist",
    "build": "run-p build:*",
    "build:common": "tsc",
    "build:esm": "tsc -p tsconfig.esm.json",
    "minify": "run-p minify:*",
    "minify:common": "terser dist/index.js -o dist/index.js -c -m",
    "minify:esm": "terser dist/esm/index.js -o dist/esm/index.js -c -m",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint --fix .",
    "lint:prettier": "prettier --write .",
    "test": "jest",
    "test:badges": "npm test -- --coverage && jest-coverage-badges",
    "prepare": "git config --local core.hooksPath .githooks"
  },
  "devDependencies": {
    "@jest/types": "^28.1.0",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.34",
    "@typescript-eslint/eslint-plugin": "5.25.0",
    "@typescript-eslint/parser": "5.25.0",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-regexp": "1.7.0",
    "jest": "^27.5.1",
    "jest-coverage-badges": "^1.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.6.2",
    "rimraf": "^3.0.2",
    "terser": "5.13.1",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  }
}
